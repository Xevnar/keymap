default
xkb_keycodes "base" {
    include "xev_includes(number)"
    include "xev_includes(top)"
    include "xev_includes(home)"
    include "xev_includes(bottom)"
    include "xev_includes(auxiliary)"
};

xkb_keycodes "function" {
    alias <FR01> = <ESC>;
    alias <FR02> = <FK01>;
    alias <FR03> = <FK02>;
    alias <FR04> = <FK03>;
    alias <FR05> = <FK04>;
    alias <FR06> = <FK05>;
    alias <FR07> = <FK06>;
    alias <FR08> = <FK07>;
    alias <FR09> = <FK08>;
    alias <FR10> = <FK09>;
    alias <FR11> = <FK10>;
    alias <FR12> = <FK11>;
    alias <FR13> = <FK12>;
};

