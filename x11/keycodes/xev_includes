xkb_keycodes "auxiliary" {
    // Modifiers
    alias <AX00> = <LSGT>;
    alias <AX01> = <LFSH>;
    alias <AX02> = <RTSH>;
    alias <AX03> = <LCTL>;
    alias <AX04> = <RCTL>;
    alias <AX05> = <LALT>;
    alias <AX06> = <RALT>;
    alias <AX07> = <LWIN>;

    // Normal keys
    alias <AX08> = <RTRN>;
    alias <AX09> = <BKSL>;
    alias <AX10> = <SPCE>;
    alias <AX11> = <TAB>;
    alias <AX12> = <CAPS>;
};

xkb_keycodes "arrows" {
    alias <AK01> = <UP>;
    alias <AK02> = <LEFT>;
    alias <AK03> = <DOWN>;
    alias <AK04> = <RGHT>;
};

xkb_keycodes "nav" {
    alias <NV01> = <INS>;
    alias <NV02> = <HOME>;
    alias <NV03> = <PGUP>;
    alias <NV04> = <DELE>;
    alias <NV05> = <END>;
    alias <NV06> = <PGDN>;
};

xkb_keycodes "keypad" {
    alias <NP01> = <NMLK>;
    alias <NP02> = <KPDV>;
    alias <NP03> = <KPMU>;
    alias <NP04> = <KPSU>;
    alias <NP05> = <KPAD>;
    alias <NP06> = <KPEN>;
    alias <NP07> = <KP7>;
    alias <NP08> = <KP8>;
    alias <NP09> = <KP9>;
    alias <NP10> = <KP4>;
    alias <NP11> = <KP5>;
    alias <NP12> = <KP6>;
    alias <NP13> = <KP1>;
    alias <NP14> = <KP2>;
    alias <NP15> = <KP3>;
    alias <NP16> = <KP0>;
    alias <NP17> = <KPDL>;
};  // vim:ft=c
