    alias <NR01> = <TLDE>;
    alias <NR02> = <AE01>;
    alias <NR03> = <AE02>;
    alias <NR04> = <AE03>;
    alias <NR05> = <AE04>;
    alias <NR06> = <AE05>;
    alias <NR07> = <AE06>;
    alias <NR08> = <AE07>;
    alias <NR09> = <AE08>;
    alias <NR10> = <AE09>;
    alias <NR11> = <AE10>;
    alias <NR12> = <AE11>;
    alias <NR13> = <AE12>;
    alias <NR14> = <BKSP>;
};

